;   param=value ; assign values to parameters

config_version=5

[addons]

metroidvania_system/settings_file="res://SampleProject/Resources/Settings.tres"

[application]

config/name="Minitroid"
config/version="0.5.0"
config/tags=PackedStringArray("addon")
run/main_scene="res://Menus/scenes/opening/opening_with_logo.tscn"
config/features=PackedStringArray("4.3")
boot_splash/show_image=false
boot_splash/image="res://splash_screen1.png"
config/icon="res://icon.png"

[autoload]

MetSys="*res://addons/MetroidvaniaSystem/Nodes/Singleton.tscn"
AppConfig="*res://addons/maaacks_game_template/base/scenes/autoloads/app_config.tscn"
SceneLoader="*res://addons/maaacks_game_template/base/scenes/autoloads/scene_loader.tscn"
ProjectUISoundController="*res://addons/maaacks_game_template/base/scenes/autoloads/project_ui_sound_controller.tscn"
GlobalVars="*res://scene_templates/global_vars.tscn"

[debug]

gdscript/warnings/unassigned_variable=0
gdscript/warnings/unassigned_variable_op_assign=0
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/window_width_override=1440
window/size/window_height_override=810
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"
window/per_pixel_transparency/allowed=true

[editor]

script/search_in_file_extensions=PackedStringArray("gd", "gdshader", "tscn")
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/MetroidvaniaSystem/plugin.cfg", "res://addons/maaacks_game_template/plugin.cfg", "res://addons/virtual_joystick/plugin.cfg")

[filesystem]

import/blender/enabled=false

